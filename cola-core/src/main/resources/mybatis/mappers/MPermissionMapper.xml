<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yus.core.dao.MPermissionDao">

    <!-- all_columns应该是全部列 -->
    <sql id="all_columns">
    `id`,
    `permission_name`,
    `permission_type`,
    `permission_system`,
    `father_id`,
    `permission_path`,
    `sort`,
    `permission_img`,
    `creator_id`,
    `creator`,
    `create_date`,
    `modifier_id`,
    `modifier`,
    `modify_date`,
    `is_valid`
    </sql>
    <!-- insert_columns应该是全部没有默认值的列 -->
    <sql id="insert_columns">
    `id`,
    `permission_name`,
    `permission_type`,
    `permission_system`,
    `permission_path`,
    `permission_img`,
    `creator_id`,
    `creator`,
    `modifier_id`,
    `modifier`
    </sql>
    <!-- 基本的DAO操作结束 -->

</mapper>